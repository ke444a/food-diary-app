{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],

  "overrides": [],

  "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
  },
  "plugins": ["react"],
  "rules": {
    "no-unused-vars": "warn",
    "react/function-component-definition": "off",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "indent": ["error", 4]
  }
}
